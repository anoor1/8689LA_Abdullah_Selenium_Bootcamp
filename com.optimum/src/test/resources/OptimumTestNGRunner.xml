<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Optimum" verbose="1" >

    <listeners>
        <listener class-name="listeners.AnnotationTransformer"/>
    </listeners>

    <parameter name="browser" value="chrome"/>
    <parameter name="url" value="http://optimum.com"/>

    <test name="test">
        <classes>
            <class name="testdeals.TestDeals"/>
            <class name="testaddress.TestAddress"/>
            <class name="testaddtocart.TestAddToCart"/>
            <class name="testchatwithus.TestChatWithUs"/>
            <class name="testcheckallphones.TestCheckAllPhones"/>
        </classes>
    </test>
</suite>
